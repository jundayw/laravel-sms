{
    "name": "jundayw/laravel-sms",
    "description": "laravel sms",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "alibabacloud/dysmsapi-20170525": "^2.0",
        "tencentcloud/sms": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Jundayw\\SMS\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jundayw\\SMS\\SMSServiceProvider"
            ],
            "aliases": {
                "SMS": "Jundayw\\SMS\\Facades\\SMS"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
